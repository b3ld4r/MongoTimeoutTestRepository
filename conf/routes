# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST    /keyboards                  controllers.KeyboardController.createKeyboard()
GET     /keyboards                  controllers.KeyboardController.getAll()

PUT     /keyboards/:id              controllers.KeyboardController.updateKeyboard(id:int)
GET     /keyboards/:id              controllers.KeyboardController.getKeyboard(id:int)
DELETE  /keyboards/:id              controllers.KeyboardController.deleteKeyboard(id:int)
