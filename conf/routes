# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST    /keyboards                  controllers.KeyboardController.createKeyboard(brand)
GET     /keyboards                  controllers.KeyboardController.getAll()

PUT     /keyboards/:id              controllers.KeyboardController.updateKeyboard(id:Long, brand)
GET     /keyboards/:id              controllers.KeyboardController.getKeyboard(id:Long)
DELETE  /keyboards/:id              controllers.KeyboardController.deleteKeyboard(id:Long)
